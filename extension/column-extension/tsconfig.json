{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "files": [],
  "include": [],
  "paths": {
    "@easy-access/column-extension": [
      "extension/column-extension/src/index.ts"
    ],
    "@easy-access/hooks": ["hooks/src/index.ts"],
    // "@easy-access/resume-editor": ["resume-editor/src/index.ts"],
    "@easy-access/utils": ["utils/src/index.ts"]
  },
  "references": [
    {
      "path": "./tsconfig.lib.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
