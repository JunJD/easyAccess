{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@easy-access/column-extension": [
        "extension/column-extension/src/index.ts"
      ],
      "@easy-access/commands-extension": [
        "extension/commands-extension/src/index.ts"
      ],
      "@easy-access/hooks": ["hooks/src/index.ts"],
      "@easy-access/spacer-extension": [
        "extension/spacer-extension/src/index.ts"
      ],
      "@easy-access/superTable-extension": [
        "extension/superTable-extension/src/index.ts"
      ],
      "@easy-access/utils": ["utils/src/index.ts"],
      "@easy-access/wavtools": ["wavtools/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
